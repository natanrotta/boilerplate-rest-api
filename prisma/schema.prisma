generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  name       String
  email      String   @unique
  password   String
  phone      String   @unique
  document   String   @unique

  @@map("user")
}
